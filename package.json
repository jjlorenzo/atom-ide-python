{
  "name": "atom-ide-python",
  "main": "main.js",
  "version": "0.1.1",
  "description": "Python support for Atom IDE.",
  "type": "module",
  "keywords": [
    "atom-ide",
    "autocomplete",
    "ide",
    "language-client",
    "language-server-protocol",
    "languageserver",
    "python",
    "pyright"
  ],
  "repository": "https://github.com/jjlorenzo/atom-ide-python",
  "license": "MIT",
  "engines": {
    "atom": ">=1.0.0 <2.0.0"
  },
  "enhancedScopes": [
    "source.python"
  ],
  "consumedServices": {
    "atom-ide-busy-signal": {
      "versions": {
        "0.1.0": "consumeBusySignal"
      }
    },
    "linter-indie": {
      "versions": {
        "2.0.0": "consumeLinterV2"
      }
    },
    "datatip": {
      "versions": {
        "0.1.0": "consumeDatatip"
      }
    },
    "signature-help": {
      "versions": {
        "0.1.0": "consumeSignatureHelp"
      }
    }
  },
  "providedServices": {
    "autocomplete.provider": {
      "versions": {
        "2.0.0": "provideAutocomplete"
      }
    },
    "code-highlight": {
      "versions": {
        "0.1.0": "provideCodeHighlight"
      }
    },
    "code-format.range": {
      "versions": {
        "0.1.0": "provideCodeFormat"
      }
    },
    "definitions": {
      "versions": {
        "0.1.0": "provideDefinitions"
      }
    },
    "find-references": {
      "versions": {
        "0.1.0": "provideFindReferences"
      }
    },
    "outline-view": {
      "versions": {
        "0.1.0": "provideOutlines"
      }
    }
  },
  "dependencies": {
    "atom-languageclient": "^1.6.0",
    "pyright": "^1.1.126"
  },
  "devDependencies": {}
}
